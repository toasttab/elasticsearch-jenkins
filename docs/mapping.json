{
    "<name-of-type>": {
        "_timestamp": {
            "enabled": true,
            "store": true
        },
        "properties": {
            "duration": {
                "type": "long"
            },
            "jobName": {
                "index": "not_analyzed",
                "type": "string"
            },
            "number": {
                "type": "long"
            },
            "result": {
                "index": "not_analyzed",
                "type": "string"
            },
            "startTime": {
                "format": "dateOptionalTime",
                "type": "date"
            },
            "queueTime": {
                "type": "long"
            }
        }
    }
}
